{
  "extends": "airbnb",
  "env": {
    "browser":  true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "react/prop-types": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-wrap-multilines": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
